<?xml version="1.0" encoding="iso-8859-1"?>
<Inst N="sequencemain" T="DataDrivenSequence">
	<Inst N="transition" T="DataDrivenTransition">
		<Attr N="PreviousAnim">
		<![CDATA[{
		"Fade":[
		{"Linear1D":[0.5,0,-16,"/theRenderingScreen->Brightness",0]},
		{ "SetValue1D" : [0.0,0.0,"Show" ]}
		]
		}
		]]>
		</Attr>
		<Attr N="NextAnim">
		<![CDATA[{
		"Fade":[
		{ "SetValue1D" : [0.0,0.0,"Show" ]},
		{ "Wait" : [  0.5 ] },
		{ "SetValue1D" : [0.0,1.0,"Show" ]},
		{"Linear1D":[0.5,-16,0,"/theRenderingScreen->Brightness",0]},
		{ "Notification" :  [ 0 , "InitScreen" ] }
		]
		}
		]]>
		</Attr>
	</Inst>
	<Inst N="UILayer" T="UI2DLayer">
		<Attr N="SizeY" V="800"/>
		<Attr N="SizeX" V="1280"/>
		<Attr N="Priority" V="50"/>
		<Attr N="RenderingScreen" V="RenderingScreen:theRenderingScreen"/>
		<Inst N="Interface" T="UIItem">
			<Attr N="SizeY" V="800"/>
			<Attr N="SizeX" V="1280"/>
			<Inst N="background" T="UIPanel">
				<Attr N="SizeY" V="800"/>
				<Attr N="SizeX" V="1280"/>
				<Attr N="Priority" V="50"/>
				<Attr N="Anchor" V="{0.500000,0.500000}"/>
				<Attr N="Dock" V="{0.500000,0.500000}"/>
				<Attr N="Color" V="{0.0, 0.2, 0.4, 1.0}"/>
			</Inst>
		</Inst>
	</Inst>
	<Inst N="Scene" T="Scene3D">
		<Inst N="genericShader" T="API3DGenericMeshShader"/>
		<Inst N="theLight1" T="Light">
			<Attr N="DiffuseColor" V="{0.7,0.7,0.7}" />
			<Attr N="Offset" V="{118.0,-230.0,600.0}" />
			<Attr N="LightType" V="DIRECTIONAL" />
			<Attr N="IsDirectional" V="true" />
			<Attr N="AmbiantColor" V="{0.3, 0.3, 0.3}" />
			<Attr N="SpecularColor" V="{0.05, 0.05, 0.05}" />
			<Attr N="LinAttenuation" V="0.0" />
			<Attr N="QuadAttenuation" V="0.0" />						
		</Inst>
		<Attr T="int" N="Priority" V="25" />
		<Inst N="camera" T="Camera">
		  <Attr T="reference" N="RenderingScreen" V="RenderingScreen:theRenderingScreen" />
		  <Attr T="bool" N="ClearColorBuffer" V="false" />
		  <Attr T="float" N="FarPlane" V="100.0" />
		  <Attr T="array" N="UpVector" V="{0,0,1}" />
		  <Attr T="array" N="ViewVector" V="{0,1,0}" />
		  <Attr N="Priority" V="1" />
		  <Attr N="VerticalFOV" V="46.6" />
		</Inst>
		<Inst N="ScaleNode" T="CoordinateSystem">
		  <Attr N="Scale" V="0.1" />
		  <Attr N="PositionY" V="1.5" />
		  <Attr N="PositionZ" V="-0.2" />
		  <Inst N="frog" P="Tree_frog.xml">
			<Rel P="tree_frog">
				<Rel P="frogMesh">
					<Inst N="animatedObject" T="AObject">
						<Inst N="skl" T="AObjectSkeletonResource">
							<Attr N="SkeletonFileName" V="Tree_frog_tree_frog.skl" />
						</Inst>
					</Inst>
				</Rel>
			</Rel>
		  </Inst>
		</Inst>
	</Inst>
</Inst>
